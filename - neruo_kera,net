🧠 KeraPulse.rs – Telekinetic Signal Converter (Rust)

pub fn convert_brainwave_to_signal(freq: f32) -> String {
    match freq {
        f if f < 4.0 => "delta_sync".to_string(),
        f if f < 8.0 => "theta_cast".to_string(),
        f if f < 12.0 => "alpha_visualize".to_string(),
        f if f < 30.0 => "beta_intent".to_string(),
        _ => "gamma_override".to_string(),
    }
}


